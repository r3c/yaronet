{
	"modifiers": [
		{
			"pattern": "\\.dist$",
			"filter": ""
		},
		{
			"pattern": "\\.inc\\.less$",
			"filter": "",
			"link": "find . -name '*.less' | xargs grep -Fl \"$(basename '{}')\" || true"
		},
		{
			"pattern": "\\.min\\.css$"
		},
		{
			"pattern": "\\.css$",
			"modify": "lessc --clean-css '{}'"
		},
		{
			"pattern": "\\.min\\.js$"
		},
		{
			"pattern": "\\.js$",
			"modify": "uglifyjs --compress --mangle -- '{}'"
		},
		{
			"pattern": "(.*)\\.less$",
			"modify": "lessc --clean-css '{}'",
			"link": "find . -name '*.less' | xargs grep -Fl \"$(basename '{}')\" || true",
			"rename": "\\1.css"
		}
	]
}
